{% extends '::base.html.twig' %}

{% block body -%}
    <h1>career</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Groupname</th>
                <td>{{ entity.groupname }}</td>
            </tr>
            <tr>
                <th>Jobs</th>
                <td>{{ entity.jobs }}</td>
            </tr>
            <tr>
                <th>Skillinterests</th>
                <td>{{ entity.skillInterests }}</td>
            </tr>
            <tr>
                <th>Profile</th>
                <td>{{ entity.profile }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ entity.url }}</td>
            </tr>
            <tr>
                <th>Popular</th>
                <td>{{ entity.popular }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('career') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('career_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
